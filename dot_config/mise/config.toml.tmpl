[tools]
{{- if eq .usage "eagle" }}
    node = 'lts'
    python = ['3.10, 3.8, 3.12']
    java = ['17, '11', 'lts']
    terraform = 'latest'
{{ else }}
    node = 'lts'
    python = '3.12'
    java = 'lts'
    ruby = '3'
    go = 'lts'
{{- end }}

[settings]
python_venv_auto_create = true
trusted_config_paths = [
    '~/Workspaces',
]
