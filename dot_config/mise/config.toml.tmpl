[tools]
{{- if eq .usage "eagle" }}
    node = 'lts'
    python = ['3.10', '3.8', '3.12']
    java = ['lts', 'adoptopenjdk-17, 'adoptopenjdk-11' ]
    terraform = 'latest'
{{ else }}
    node = 'lts'
    python = '3.12'
    java = 'lts'
    ruby = '3'
{{- end }}

[settings]
python_venv_auto_create = true
trusted_config_paths = [
    '~/Workspaces',
]
